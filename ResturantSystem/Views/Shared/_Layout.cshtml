
<!DOCTYPE html>
<html lang="en">
<!-- Basic -->
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Site Metas -->
    <title>Yamifood Restaurant</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Site Icons -->
    <link rel="shortcut icon" href="~/Content/resturant/images/favicon.ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="~/Content/resturant/images/apple-touch-icon.png">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/Content/resturant/css/bootstrap.min.css">
    <!-- Site CSS -->
    <link rel="stylesheet" href="~/Content/resturant/css/style.css">
    <!-- Responsive CSS -->
    <link rel="stylesheet" href="~/Content/resturant/css/responsive.css">
    <!-- Custom CSS -->
    <link rel="stylesheet" href="~/Content/resturant/css/custom.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-0evHe/X+R7YkIZDRvuzKMRqM+OrBnVFBL6DOitfPri4tjfHxaWutUpFmBp4vmVor" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />

    @*<link href="http://maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css" />*@
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <script type="text/javascript" src="http://maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery-1.7.1.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.validate.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script type="text/javascript" src="~/Scripts/passwordscheck.js"></script>
    <link href="~/Scripts/passwordscheck.css" rel="stylesheet" />

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>
    <!-- Start header -->
    <header class="top-navbar">
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <div class="container">
                @{
                    var info = Session["UserName"];
                }
                <i class="bi bi-person-badge-fill"></i>

                <a class="navbar-brand" href="@Url.Action("Index", "Home")">
                    @if (@info != null && @info.ToString() != "")
                    {
                        @info
                    }
                    <img src="~/Content/resturant/images/logo.png" alt="" />
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbars-rs-food" aria-controls="navbars-rs-food" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbars-rs-food">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item"><a class="nav-link" href="@Url.Action("Index", "Home")">Home</a></li>
                        <li class="nav-item"><a class="nav-link" href="@Url.Action("Resmenu", "Resmenu")">Menu</a></li>
                        <li class="nav-item"><a class="nav-link" href="@Url.Action("About", "About")">About</a></li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="dropdown-a" data-toggle="dropdown">Pages</a>
                            <div class="dropdown-menu" aria-labelledby="dropdown-a">
                                <a class="dropdown-item" href="@Url.Action("Reservation", "Reservation")">Reservation</a>
                                <a class="dropdown-item" href="@Url.Action("Gallery", "Gallery")">Gallery</a>
                            </div>
                        </li>
                        <li class="nav-item"><a class="nav-link" href="@Url.Action("Contact","Contact")">Contact</a></li>
                         @if (@info == null || @info.ToString() == "")
                        {
                            <li class="nav-item"><a class="nav-link" href="@Url.Action("Index", "UserLogin")">Login</a></li>
                        }
                        <li class="nav-item">
                            @if (Session["Status"] == "1")
                            {
                                <a class="nav-link" href="@Url.Action("Index","AddToCart")">
                                    <span class="fa fa-shopping-cart fa-1x">
                                        <span class="text-danger ">@ViewBag.cart</span>
                                    </span>
                                </a>

                            }
                            else
                            {
                                <a class="nav-link" href="@Url.Action("Index","AddToCart")">
                                    <span class="fa fa-shopping-cart fa-1x">
                                     
                                    </span>
                                </a>
                          
                            }
                        </li>
                         @if (@info == null || @info.ToString() == "")
                        {
                            <li class="nav-item"><a class="nav-link" href="@Url.Action("Index", "UserLogin")">Login</a></li>
                        }
                       
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    @RenderBody()


    <!-- Start Footer -->
    <footer class="footer-area bg-f">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-6">
                    <h3>About Us</h3>
                    <p>Integer cursus scelerisque ipsum id efficitur. Donec a dui fringilla, gravida lorem ac, semper magna. Aenean rhoncus ac lectus a interdum. Vivamus semper posuere dui, at ornare turpis ultrices sit amet. Nulla cursus lorem ut nisi porta, ac eleifend arcu ultrices.</p>
                </div>
                <div class="col-lg-4 col-md-6">
                    <h3>Opening hours</h3>

                    <p><span class="text-color">Sun-Mon :</span> 8:Am - 10PM</p>
                    <p><span class="text-color">Tue-Wed :</span> 8:Am - 10PM</p>
                    <p><span class="text-color">Thu-Fri :</span> 9:Am - 9PM</p>
                    <p><span class="text-color">Sat :</span> 7:Am - 11PM</p>
                </div>
                <div class="col-lg-4 col-md-6">
                    <h3>Contact information</h3>
                    <p class="lead">Hamro Nagar,  Chakrapath, Kathmandu, Nepal, 508000</p>
                    <p class="lead"><a href="#">+977 9804873652</a></p>
                    <p class="lead"><a href="#">+977 9856643213</a></p>
                    <p><a href="#"> info@admin.com</a></p>
                </div>

            </div>
        </div>

        <div class="copyright">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <p class="company-name">
                            All Rights Reserved. &copy; 2018 <a href="#">Yamifood Restaurant</a>
                            @*<a href="https://html.design/">html design</a>*@
                        </p>
                    </div>
                </div>
            </div>
        </div>

    </footer>
    <!-- End Footer -->

    <a href="#" id="back-to-top" title="Back to top" style="display: none;">&uarr;</a>

    <!-- ALL JS FILES -->
    <script src="~/Content/resturant/js/jquery-3.2.1.min.js"></script>
    <script src="~/Content/resturant/js/popper.min.js"></script>
    <script src="~/Content/resturant/js/bootstrap.min.js"></script>
    <!-- ALL PLUGINS -->
    <script src="~/Content/resturant/js/jquery.superslides.min.js"></script>
    <script src="~/Content/resturant/js/images-loded.min.js"></script>
    <script src="~/Content/resturant/js/isotope.min.js"></script>
    <script src="~/Content/resturant/js/baguetteBox.min.js"></script>
    <script src="~/Content/resturant/js/form-validator.min.js"></script>
    <script src="~/Content/resturant/js/contact-form-script.js"></script>
    <script src="~/Content/resturant/js/custom.js"></script>
</body>
</html>
